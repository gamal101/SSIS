using System.IO;
#endregion

namespace ST_82bb70268a07409490000f9677b617f9
{
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
		public void Main()
		{
            // get the name/path for this file
            string thisFilePath = Dts.Variables["FilePath"].Value.ToString();

            // get a reference to this
            FileInfo thisFile = new FileInfo(thisFilePath);

            // get size of file
            long fileSize = thisFile.Length;

            // add to current total
            Dts.Variables["CumulativeSize"].Value =
                Convert.ToInt32(Dts.Variables["CumulativeSize"].Value) + 
                fileSize;

		}

